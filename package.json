  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coveragePathIgnorePatterns": [
      "/src/store/AppStore.ts",
      "/src/store/models/Orm.ts",
      "/src/index.tsx",
      "(\\w.*)Container.[tj]s"
    ]
